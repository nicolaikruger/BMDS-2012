import org.jgroups.*;

// Ad-hoc class biatch
public class TaskGroup extends ReceiverAdapter{
	private static JChannel channel = initChannel("TaskGroupKolera");
	
	private static JChannel initChannel(String taskGroupName){
		JChannel tmpChannel = null;
		try{
		tmpChannel = new JChannel(taskGroupName);
		}catch(Exception e){
			System.out.println("WHATAFAK :O!");	
		}
		
		return tmpChannel;
	}
	
	public TaskGroup(){
		
	}	
}
