import org.jgroups.*;

public class TaskGroup {
	private JChannel channel = initChannel("TaskGroupKolera");
	
	private JChannel initChannel(String taskGroupName){
		JChannel tmpChannel = null;
		try{
		tmpChannel = new JChannel(taskGroupName);
		}catch(Exception e){
			System.out.println("WHATAFAK :O!");	
		}
		
		return tmpChannel;
	}
	
	
}
